{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out",
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./src/typings"],
      "ast-compare": ["node_modules/ast-compare/dist/ast-compare.esm.js"],
      "ast-contains-only-empty-space": [
        "node_modules/ast-contains-only-empty-space/dist/ast-contains-only-empty-space.esm.js"
      ],
      "node_modules/ast-get-object": [
        "node_modules/ast-get-object/dist/ast-get-object.esm.js"
      ],
      "ast-get-values-by-key": [
        "node_modules/ast-get-values-by-key/dist/ast-get-values-by-key.esm.js"
      ],
      "node_modules/ast-monkey-traverse": [
        "node_modules/ast-monkey-traverse/dist/ast-monkey-traverse.esm.js"
      ]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "out"]
}
